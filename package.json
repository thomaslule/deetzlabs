    "dev": "nodemon"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaslule/deetzlabs.git"
  },
  "author": "thomaslule",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thomaslule/deetzlabs/issues"
  },
  "homepage": "https://github.com/thomaslule/deetzlabs#readme",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.1.0",
    "copyfiles": "^2.1.0",
    "husky": "^1.3.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "1.15.3",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.4",
    "ts-node": "^8.6.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/lodash.mapvalues": "^4.6.4",
    "@types/morgan": "^1.7.35",
    "@types/node-fetch": "^2.5.5",
    "@types/pg": "^7.4.10",
    "@types/pg-query-stream": "^1.0.2",
    "@types/socket.io": "^1.4.38",
    "@types/stream-to-array": "^2.3.0",
    "body-parser": "^1.18.3",
    "deetzlabs-web": "^0.4.0",
    "es-objects": "^0.1.2",
    "express": "^4.16.3",
    "express-http-proxy": "^1.4.0",
    "express-validator": "^5.3.0",
    "lodash.mapvalues": "^4.6.0",
    "logform": "^1.10.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "ow": "^0.8.0",
    "pg": "^7.5.0",
    "pg-query-stream": "^1.1.1",
    "socket.io": "^2.1.1",
    "stream-to-array": "^2.3.0",
    "twitch-channel": "^0.4.4",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.2"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "test"
    ],
    "exec": "ts-node --transpile-only ./test/develop",
    "ext": "ts"
  }
}
