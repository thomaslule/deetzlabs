{
  "name": "deetzlabs",
  "version": "2.1.0",
  "description": "Add viewers achievements and other widgets to a twitch stream",
  "main": "lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rimraf ./lib && tsc && copyfiles -u 1 \"./src/widgets/public/**/*\" ./lib",
